# Gemini AI API Key
# Get your API key from: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_key_here

# Supabase Configuration (if INCLUDE_SUPABASE is true)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Additional API Keys
VITE_OPENAI_API_KEY=your_openai_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_key_here

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_HOTJAR_ID=your_hotjar_id_here

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Development Settings
VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3001
VITE_DEBUG=true

# Storage Configuration (if using cloud storage)
VITE_AWS_REGION=us-east-1
VITE_S3_BUCKET=your-bucket-name

# Third-party Integrations
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id_here

# Security
VITE_APP_SECRET=your_app_secret_here
VITE_JWT_SECRET=your_jwt_secret_here

# Email Service (for notifications)
VITE_SMTP_HOST=smtp.example.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=your_email@example.com
VITE_SMTP_PASS=your_email_password

# Content Moderation
VITE_CONTENT_MODERATION_API_KEY=your_moderation_api_key_here

# Translation Service
VITE_GOOGLE_TRANSLATE_API_KEY=your_translate_api_key_here

# OCR Service
VITE_TESSERACT_WORKER_PATH=/path/to/tesseract/worker
VITE_AZURE_COGNITIVE_SERVICES_KEY=your_azure_key_here

# Video Processing
VITE_FFMPEG_WORKER_PATH=/path/to/ffmpeg/worker
VITE_VIDEO_API_KEY=your_video_api_key_here

# Sign Language
VITE_SIGN_LANGUAGE_API_KEY=your_sign_language_api_key_here

# LMS Integration
VITE_CANVAS_API_KEY=your_canvas_api_key_here
VITE_BLACKBOARD_API_KEY=your_blackboard_api_key_here
VITE_MOODLE_API_KEY=your_moodle_api_key_here

# Performance Monitoring
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_LOGROCKET_APP_ID=your_logrocket_app_id_here

# A/B Testing
VITE_OPTIMIZELY_SDK_KEY=your_optimizely_key_here
VITE_LAUNCHDARKLY_CLIENT_ID=your_launchdarkly_client_id_here

# Accessibility Testing
VITE_AXE_API_KEY=your_axe_api_key_here
VITE_WAVE_API_KEY=your_wave_api_key_here